<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.AttendeeMenuController">
   <children>
      <Button fx:id="goBackButton" layoutX="12.0" layoutY="11.0" mnemonicParsing="false" onAction="#goBackButtonOnAction" text="Go Back" />
      <VBox layoutX="167.0" layoutY="124.0" prefHeight="150.0" prefWidth="267.0">
         <children>
            <Label fx:id="welcomeLabel" text="Welcome">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Button fx:id="browseButton" mnemonicParsing="false" onAction="#browseButtonOnAction" prefHeight="27.0" prefWidth="296.0" text="Browse Events">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="seeSignedUpButton" mnemonicParsing="false" onAction="#seeSignedUpButtonOnAction" prefHeight="27.0" prefWidth="299.0" text="See Signed Up Events">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="seeMessageButton" mnemonicParsing="false" onAction="#seeMessageAction" prefHeight="27.0" prefWidth="267.0" text="Message">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="requestsButton" mnemonicParsing="false" onAction="#handleRequestButton" prefHeight="27.0" prefWidth="316.0" text="User Requests" />
         </children></VBox>
   </children>
</AnchorPane>
