<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.RequestController">
  <center>
    <AnchorPane BorderPane.alignment="CENTER">
      <children>
            <Button fx:id="goBackButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBackButtonOnAction" text="Go Back" />
            <Button fx:id="newRequestButton" layoutX="387.0" layoutY="355.0" mnemonicParsing="false" onAction="#newRequestButtonOnAction" prefHeight="25.0" prefWidth="160.0" text="Create New Request" />
            <TableView fx:id="requestTable" layoutX="58.0" layoutY="48.0" prefHeight="302.0" prefWidth="485.0">
              <columns>
                <TableColumn fx:id="requestColumn" prefWidth="425.0" text="Your Request" />
                  <TableColumn fx:id="statusColumn" prefWidth="75.0" text="Status" />
              </columns>
            </TableView>
      </children>
    </AnchorPane>
  </center>
</BorderPane>
