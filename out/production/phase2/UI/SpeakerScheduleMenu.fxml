<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="UI.SpeakerScheduleMenuController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Button fx:id="goBackButton" layoutY="1.0" mnemonicParsing="false" onAction="#goBackButtonOnAction" text="Go Back" />
        <DatePicker fx:id="datePicker" layoutX="15.0" layoutY="40.0" promptText="Pick a Date" showWeekNumbers="true" />
        <ComboBox fx:id="roomFilterComboBox" layoutX="225.0" layoutY="40.0" prefWidth="150.0" promptText="Pick a Room" />
        <ComboBox fx:id="eventTypeFilterComboBox" layoutX="398.0" layoutY="40.0" prefWidth="150.0" promptText="Pick an Event Type" />
    </children>

</AnchorPane>
