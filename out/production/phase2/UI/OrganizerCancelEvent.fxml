<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="250.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.OrganizerCancelEventController">
    <center>
        <AnchorPane BorderPane.alignment="CENTER">
            <children>
                <ComboBox fx:id="eventNameComboBox" layoutX="106.0" layoutY="76.0" promptText="Select the event you want to cancel" />
                <Button fx:id="cancelEventButton" layoutX="197.0" layoutY="139.0" mnemonicParsing="false" onAction="#cancelEventButtonOnAction" text="Cancel" />
                <Label fx:id="eventNameErrorLabel" layoutX="196.0" layoutY="130.0" />
                <Button fx:id="goBackButton" layoutX="6.0" layoutY="2.0" mnemonicParsing="false" onAction="#goBackButtonOnAction" text="Go Back" />
                <Label fx:id="cancelEventSuccessLabel" layoutX="206.0" layoutY="130.0" />
            </children>
        </AnchorPane>
    </center>

</BorderPane>
